programa
{
	inclua biblioteca Matematica
	inclua biblioteca Util
	
	funcao inicio()
	{
		cadeia a 
		cadeia resposta
		inteiro dinheiro = 1000
		inteiro aposta
		inteiro valor = 0
		inteiro result = 0
		inteiro sort
		inteiro sort2
		inteiro sort3
		inteiro dig1=0, dig2=0, dig3=0
		inteiro sete = 7
		inteiro Bar = 4
		inteiro BarBar = 5
		inteiro BarBarBar = 6
		inteiro diamante = 10
		
		escreva("!Bem Vindo ao Cassino! \n")
		escreva("Pressione ENTER para começar \n")
		leia(resposta) 
		se(resposta == ""){
		escreva("Você começou em um caça niquel com 1000 reais. \n")
		escreva("vc deseja apostar quanto? ")
		leia (aposta)
		se(aposta <= dinheiro){
		dinheiro -= aposta
		sort = sorteia (1, 5)
		sort2 = sorteia (1, 5)
		sort3 = sorteia (1, 5)
			para(inteiro i = 1; i <= 5; i++){
		sort = sorteia (1, 5)
		
				se(sort == 1){
				valor+=7
				dig1 = sete
				escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
					sort2 = sorteia(1, 5)
					se(sort2 == 1){
					dig2 = sete
					escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")		
							sort3 = sorteia(1, 5)
							se(sort3 == 1){
							dig3 = sete
							result = result + dig1 + dig2 + dig3
								se (result <= 10){ 
								dinheiro -= aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce perdeu", aposta, ", ficou com", dinheiro, "e depois desistiu.")
								}
								se (result == 10){
								dinheiro += aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce nem perdeu e nem ganhou, continuou com 1000 e broxou de continuar apostando.") 
								}
								se (result >= 15){
								dinheiro += aposta * 2
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final bom, voce ganhou", aposta, ", e ficou com", dinheiro, "e foi expulso por ganhar (quem tem que ganhar é sempre o estabelecimento).")
							}
							pare
							}
							sort3 = sorteia(1, 5)
							se(sort3 == 2){
							dig3 = Bar
							result = result + dig1 + dig2 + dig3
								se (result <= 10){ 
								dinheiro -= aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce perdeu", aposta, ", ficou com", dinheiro, "e depois desistiu.")
								}
