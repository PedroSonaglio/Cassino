								escreva(" final normal, voce nem perdeu e nem ganhou, continuou com 1000 e broxou de continuar apostando.") 
								}
								se (result >= 15){
								dinheiro += aposta * 2
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final bom, voce ganhou", aposta, ", e ficou com", dinheiro, "e foi expulso por ganhar (quem tem que ganhar é sempre o estabelecimento).")
							}
							pare
							}
					sort2 = sorteia(1, 5)
					se(sort2 == 5)
					dig2 = diamante
					escreva("|    ", dig1, "    |    ", dig2, "   |   ", dig3, "   | \n")
							sort3 = sorteia(1, 5)
							se(sort3 == 1){
							dig3 = sete
							result = result + dig1 + dig2 + dig3
								se (result <= 10){ 
								dinheiro -= aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce perdeu", aposta, ", ficou com", dinheiro, "e depois desistiu.")
								}
								se (result == 10){
								dinheiro += aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce nem perdeu e nem ganhou, continuou com 1000 e broxou de continuar apostando.") 
								}
								se (result >= 15){
								dinheiro += aposta * 2
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final bom, voce ganhou", aposta, ", e ficou com", dinheiro, "e foi expulso por ganhar (quem tem que ganhar é sempre o estabelecimento).")
							}
							pare
							}
							sort3 = sorteia(1, 5)
							se(sort3 == 2){
							dig3 = Bar
							result = result + dig1 + dig2 + dig3
								se (result <= 10){ 
								dinheiro -= aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce perdeu", aposta, ", ficou com", dinheiro, "e depois desistiu.")
								}
								se (result == 10){
								dinheiro += aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce nem perdeu e nem ganhou, continuou com 1000 e broxou de continuar apostando.") 
								}
								se (result >= 15){
								dinheiro += aposta * 2
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final bom, voce ganhou", aposta, ", e ficou com", dinheiro, "e foi expulso por ganhar (quem tem que ganhar é sempre o estabelecimento).")
							}
							pare
							}
							sort3 = sorteia(1, 5)
							se(sort3 == 3){
							dig3 = BarBar
							result = result + dig1 + dig2 + dig3
								se (result <= 10){ 
								dinheiro -= aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce perdeu", aposta, ", ficou com", dinheiro, "e depois desistiu.")
								}
								se (result == 10){
								dinheiro += aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce nem perdeu e nem ganhou, continuou com 1000 e broxou de continuar apostando.") 
								}
								se (result >= 15){
								dinheiro += aposta * 2
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final bom, voce ganhou", aposta, ", e ficou com", dinheiro, "e foi expulso por ganhar (quem tem que ganhar é sempre o estabelecimento).")
							}
							pare
							}
							sort3 =sorteia(1, 5)
							se(sort3 == 4){
							dig3 = BarBarBar
							result = result + dig1 + dig2 + dig3
								se (result <= 10){ 
								dinheiro -= aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce perdeu", aposta, ", ficou com", dinheiro, "e depois desistiu.")
								}
								se (result == 10){
								dinheiro += aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce nem perdeu e nem ganhou, continuou com 1000 e broxou de continuar apostando.") 
								}
								se (result >= 15){
								dinheiro += aposta * 2
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final bom, voce ganhou", aposta, ", e ficou com", dinheiro, "e foi expulso por ganhar (quem tem que ganhar é sempre o estabelecimento).")
							}
							pare
							}
							sort3 = sorteia(1, 5)
							se(sort3 == 5){
							dig3 = diamante
							result = result + dig1 + dig2 + dig3
								se (result <= 10){ 
								dinheiro -= aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce perdeu", aposta, ", ficou com", dinheiro, "e depois desistiu.")
								}
								se (result == 10){
								dinheiro += aposta
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final normal, voce nem perdeu e nem ganhou, continuou com 1000 e broxou de continuar apostando.") 
								}
								se (result >= 15){
								dinheiro += aposta * 2
								escreva("|    ", dig1, "    |    ", dig2, "    |    ", dig3, "    |\n")
								escreva(" final bom, voce ganhou", aposta, ", e ficou com", dinheiro, "e foi expulso por ganhar (quem tem que ganhar é sempre o estabelecimento).")
							}
							pare
							}}}
		}se(aposta > dinheiro){
			escreva("só aceitamos fiado de qm tem 90 anos e está acompanhado dos avós. Colocaram um saco de pano na sua cabeça para te bater e te jogaram para fora do cassino. (final secreto2?)")
		}	
	}senao{
		escreva("Você não ficou viciado e não ficou com nome sujo no serasa. (!final secreto!)")
		}
	}}
}
